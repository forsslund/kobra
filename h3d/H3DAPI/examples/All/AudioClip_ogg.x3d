<?xml version="1.0" encoding="utf-8"?>
<X3D profile='Full' version='3.2'>
  <head>
    <meta name='title' content='AudioClip_ogg.x3d'/>
    <meta name='description' content='X3D AudioClip node example using ogg file sample.'/>
    <meta name='author' content='SenseGraphics AB, 2006-2019'/>
  </head>
  
  <Scene>
    <Viewpoint position='0 0 0.6' />
    <Sound maxFront="0.9" maxBack="0.9"  minFront="0.0001" intensity="1" DEF="SOUND" >
      <AudioClip DEF="AUDIO" url="sound/ding.ogg" loop="TRUE"/>
    </Sound>
  </Scene>
</X3D>