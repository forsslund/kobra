      </Shape>

      <ToggleGroup DEF='Body1_MeshToggle' graphicsOn='false' hapticsOn='false'>
        <Shape>
          <Appearance>
            <FillProperties hatched='false' filled='false' />
            <Material />
          </Appearance>
          <IndexedTriangleSet USE='Body1_Surface' />
        </Shape>
      </ToggleGroup>

      <!-- Display usage message -->
      <Transform DEF="Nr">
        <DirectionalLight direction="0 0 -1" />
        <Transform translation="0 0.037 0.5" scale="0.2 0.2 0.2">
          <Shape>
            <Appearance>
              <Material diffuseColor="0 0 0" />
            </Appearance>
            <Text DEF="Text" string='"H3DPhysics Soft Body: SoftBody"' solid="true">
              <FontStyle justify='"MIDDLE" "BEGIN"' size="0.015" spacing="1" />
            </Text>
          </Shape>
          <Transform translation="0 -0.02 0">
            <Shape>
              <Appearance>
                <Material diffuseColor="0 0 0" />
              </Appearance>
              <Text DEF="Text" string='"Touch the soft bodies with the haptic device. Press m to show/hide tetra mesh." "Press and hold device button to grab soft bodies."' solid="true">
                <FontStyle justify='"MIDDLE" "BEGIN"' size="0.01" spacing="1" />
              </Text>
            </Shape>
          </Transform>
        </Transform>
      </Transform>

      <PhysicsBodyCollection DEF='RBC' physicsEngine='Bullet' gravity='0 -1 0'>

        <!-- Bullet params: fixedTimeStep - smaller value, more stable/accurate but slower
                            worldScale - the torus mesh is scaled by 0.04, so to match bullet
                                         demo behaviour scale up worl by 25, otherwise objects 
                                         fall through each other -->
        <BulletWorldOptions fixedTimeStep='0.002' worldScale='25' />

        <!-- Geometries for collision detection -->
        <CollisionCollection containerField='collider' frictionCoefficients='0.2 0.2'>
          <CollidableShape DEF='C' containerField='collidables'>
            <Shape USE='S' containerField='shape' />
          </CollidableShape>
          <CollidableShape DEF='C1' containerField='collidables'>
            <Shape USE='S1' containerField='shape' />
          </CollidableShape>

        </CollisionCollection>

        <!-- Rigid bodies for physics simulation -->
        <RigidBody DEF='R' position='0 -0.3 0' fixed='true'>
          <CollidableShape USE='C' containerField='geometry' />
          <Box USE='G' containerField='massDensityModel' />
        </RigidBody>
        <RigidBody DEF='R1' position='0 0.09 0' orientation='1 0 0 1.57' fixed='true'>
          <CollidableShape USE='C1' containerField='geometry' />
          <Box USE='G1' containerField='massDensityModel' />
          <BulletCollidableOptions convex='true' collisionMargin='0.001' />
        </RigidBody>

        <Cloth DEF='CLOTH1'>
          <MassSpringPhysicsMaterial>

            <UniformMass mass="0.5" />
            <UniformStiffness stiffness="1.0" />
            <UniformDamping damping="0.005" />
            <UniformFriction friction="0.3" />

          </MassSpringPhysicsMaterial>
          <IndexedTriangleSet USE="Body1_Surface" />
          <!-- Bullet params: pIterations - required to stop the torus drifting along the rigid body
                              collisionMargin - reduced from the default value (0.01) to reduce gaps between 
                                                soft body and rigid bodies it collides with -->
          <BulletSoftBodyOptions pIterations='15' collisionMargin='0.001' collisionOptions='"CLUSTER_RIGIDSOFT", "CLUSTER_SOFTSOFT"' />
        </Cloth>

        <FunctionSoftBodyModifier>
          <Cloth USE='CLOTH1' containerField='body1' />
          <GaussianFunction amplitude='0.005' width='0.05' containerField='distanceToForce' />
        </FunctionSoftBodyModifier>

      </PhysicsBodyCollection>
    </Transform>

    <KeySensor DEF='KS' />

    <PythonScript url='Grabber.py' moduleName='Grabber' />
    <PythonScript url='SoftBodyDemo.py'>
      <ToggleGroup USE='Body1_MeshToggle' graphicsOn='false' containerField='references' />
      <Material USE='Body1_Mat' containerField='references' />
      <KeySensor USE='KS' containerField='references' />
      <PhysicsBodyCollection USE='RBC' containerField='references' />
      <Group USE='Root' containerField='references' />
    </PythonScript>

    <PythonScript url='GetTransforms.py' moduleName='GetTransforms' />
    <PythonScript url='SoftBodyRigidCollisionsTransforms.py'>
      <PhysicsBodyCollection USE='RBC' containerField='references' />
    </PythonScript>

    <ROUTE fromNode='R' fromField='position' toNode='T' toField='translation' />
    <ROUTE fromNode='R' fromField='orientation' toNode='T' toField='rotation' />
    <ROUTE fromNode='R1' fromField='position' toNode='T1' toField='translation' />
    <ROUTE fromNode='R1' fromField='orientation' toNode='T1' toField='rotation' />

    <!-- Update title text message position -->
    <ROUTE fromNode="V" fromField="totalPosition" toNode="PS" toField="textPos" />
    <ROUTE fromNode="V" fromField="totalOrientation" toNode="PS" toField="textPos" />
    <ROUTE fromNode="PS" fromField="textPos" toNode="Nr" toField="translation" />
    <ROUTE fromNode="V" fromField="totalOrientation" toNode="Nr" toField="rotation" />

  </Scene>
</X3D>
